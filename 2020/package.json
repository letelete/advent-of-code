{
  "name": "aoc-2020",
  "version": "1.0.0",
  "description": "My Advent of Code 2020 solutions",
  "main": "dist/index.js",
  "jest": {
    "preset": "ts-jest",
    "transform": {
      "node_modules/variables/.+\\.(j|t)sx?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!variables/.*)"
    ],
    "testRegex": ".\\.test\\.tsx?$"
  },
  "scripts": {
    "start": "rimraf dist && tsc && ts-node .",
    "test": "sh ./run_tests.sh $npm_config_day"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/letelete/advent-of-code.git"
  },
  "keywords": [
    "adventofcode"
  ],
  "author": "letelete",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/letelete/advent-of-code/issues"
  },
  "homepage": "https://github.com/letelete/advent-of-code#readme",
  "devDependencies": {
    "@types/jest": "^26.0.16",
    "@types/node": "^14.14.10",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.2"
  }
}
